    },
    "second_disable_wprocket_lazyload": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "off"
          }
        }
      }
    }
  },
  "customCssFields": {
    "toggle_button_container" :{
      "subName": "toggle_button_container",
      "selectorSuffix":" .dipi-content-toggle__button-container"
    },
    "toggle_button_slider" :{
