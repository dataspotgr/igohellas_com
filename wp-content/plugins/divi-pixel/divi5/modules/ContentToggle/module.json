{
  "name": "dipi/content-toggle",
  "d4Shortcode": "dipi_content_toggle",
  "title": "Pixel Content Toggle",
  "titles": "Pixel Content Toggles",
  "moduleIcon": "dipi/content-toggle",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "Pixel Content Toggle"
            }
          }
        },
        "advanced": {
          "text": {
            "text": {
              "desktop": {
                "value": {
                  "color": "light"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "background": {
            "desktop": {
              "value": {
                "color": "#ecf4f7"
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "first_text": {
      "type": "object",
      "inlineEditor": "plainText",
      "selector": "{{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h1, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h2, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h3, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h4, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h5, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle__first-text h6",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": ""
          }
        }
      },
      "decoration": {
        "font": {
          "font": {
            "desktop": {
              "value": {
                "headingLevel": "h5"
              }
            }
          }
        }
      }
    },

    "second_text": {
      "type": "object",
      "inlineEditor": "plainText",
      "selector":  "{{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h1, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h2, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h3, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h4, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h5, {{selector}} .dipi-content-toggle__button-container .dipi-content-toggle_second-text h6",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": ""
          }
        }
      }
    },
    "second_divi_library": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": ""
          }
        }
      }
    },
    "toggle_alignment": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "center"
          }
        }
      }
    },
    "toggle_size": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "14px"
          }
        }
      }
    },
    "first_btn_color": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "#fff"
          }
        }
      }
    },
    "first_bg_color": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
