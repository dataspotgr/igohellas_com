      "default": {
        "innerContent": {
          "desktop": {
            "value": "off"
          }
        }
      }
    },
    "second_disable_browser_lazyload": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
