    "second_content_delay": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "100ms"
          }
        }
      }
    },
    "second_content_speed": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "600ms"
          }
        }
      }
    },
    "first_disable_browser_lazyload": {
      "type": "object",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "off"
          }
        }
      }
    },
    "first_disable_wprocket_lazyload": {
      "type": "object",
      "inlineEditor": "plainText",
